class Threads implements Runnable {
   private Thread t;
   private String threadName="thread1";
   int g,h;
   String j;
   
   public void initialize(int a, int b, String c){
	   g=a;
	   h=b;
	   j=c;
	   System.out.println("threads.init path = "+c);
	    this.start();
   }
   
   public void run() {
      Searcher.newSearcher(g, h, j);
      System.out.println("threads.run");
   }
   
   
   public void start () {
       System.out.println("threads.start");
      if (t == null) {
          System.out.println("creates a thread t");
         t = new Thread (this, Searcher.abc[Path.searchers]);
         t.start ();
      }
   }
}
